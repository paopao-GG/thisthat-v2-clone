    "list:categories": "tsx scripts/list-categories.ts",
    "fix:categories": "tsx scripts/fix-categories.ts",
    "db:generate": "prisma generate --schema=prisma/schema.markets.prisma && prisma generate --schema=prisma/schema.users.prisma",
    "db:generate:markets": "prisma generate --schema=prisma/schema.markets.prisma",
    "db:generate:users": "prisma generate --schema=prisma/schema.users.prisma",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push --schema=prisma/schema.markets.prisma && prisma db push --schema=prisma/schema.users.prisma",
    "db:push:markets": "prisma db push --schema=prisma/schema.markets.prisma",
    "db:push:users": "prisma db push --schema=prisma/schema.users.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:packages": "tsx prisma/seed-packages.ts",
    "stats": "tsx scripts/show-category-stats.ts",
    "test:prefetch": "tsx scripts/test-category-prefetch.ts",
    "test:amm": "tsx scripts/test-amm-endpoints.ts"
