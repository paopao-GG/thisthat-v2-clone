  "main": "dist/app/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app/index.js",
    "dev": "tsx watch --env-file=.env src/app/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:polymarket": "tsx scripts/tests/test-polymarket-ingestion.ts",
    "test:fetch-polymarket": "tsx scripts/test-fetch-polymarket-data.ts",
